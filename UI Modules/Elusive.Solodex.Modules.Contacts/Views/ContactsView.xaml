<UserControl x:Class="Elusive.Solodex.Modules.Contacts.Views.ContactsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Ui="clr-namespace:Elusive.Solodex.UI.Common;assembly=Elusive.Solodex.UI.Common"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:prism="http://www.codeplex.com/prism"
             mc:Ignorable="d">

    <Border Margin="2"
            Background="White"
            BorderBrush="Black"
            BorderThickness="1"
            CornerRadius="0">
        <Grid>
            <!--  Main Region  -->
            <ContentControl prism:RegionManager.RegionName="{x:Static Ui:RegionNames.ContactsRegion}" />
        </Grid>
    </Border>

</UserControl>
